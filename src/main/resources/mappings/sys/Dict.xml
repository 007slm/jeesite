<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.thinkgem.jeesite.modules.sys.dao.MyBatisDictDao">
    
	<select id="get" parameterType="int" resultType="com.thinkgem.jeesite.modules.sys.entity.Dict">
		select * from sys_dict where id=#{id}
	</select>
	
	<select id="findAll" resultType="com.thinkgem.jeesite.modules.sys.entity.Dict">
		select * from sys_dict
	</select>
	
</mapper>